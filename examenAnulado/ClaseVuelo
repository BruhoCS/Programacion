package examenanulado;

import java.time.LocalDateTime;

public class Vuelo {

    String codigo;
    String origen;
    String destino;
    LocalDateTime salida;
    LocalDateTime llegada;

    public Vuelo(String codigo, String origen, String destino, LocalDateTime salida, LocalDateTime llegada) {
        this.codigo = codigo;
        this.origen = origen;
        this.destino = destino;
        this.salida = salida;
        this.llegada = llegada;
    }

    public String getCodigo() {
        return codigo;
    }

    public void setCodigo(String codigo) {
        this.codigo = codigo;
    }

    public String getOrigen() {
        return origen;
    }

    public void setOrigen(String origen) {
        this.origen = origen;
    }

    public String getDestino() {
        return destino;
    }

    public void setDestino(String destino) {
        this.destino = destino;
    }

    public LocalDateTime getSalida() {
        return salida;
    }

    public void setSalida(LocalDateTime salida) {
        this.salida = salida;
    }

    public LocalDateTime getLlegada() {
        return llegada;
    }

    public void setLlegada(LocalDateTime llegada) {
        this.llegada = llegada;
    }

    @Override
    public String toString() {
        return "Codigo: "+codigo+"\n"+"Origen: "+origen+"\n"+"Destino: "+destino+"\n"+"salida"+salida
                +"\n"+"Llegada"+llegada;
    }

    
}
